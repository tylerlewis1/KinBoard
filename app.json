{
  "expo": {
    "name": "KinBoard",
    "slug": "kinboard",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logoz.png",
    "scheme": "kinboard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tylerlewis.kinboard",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "This app needs the camera so users can take and upload profile pictures and cover images for their circles. It can also be used to scan qr codes to join circles.",
        "NSPhotoLibraryUsageDescription": "This app needs access to your photos so you can upload images to your profile and for your circle cover iamge."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tylerlewis.kinboard"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "63fd542f-1c9a-4f55-9aa4-d66bb37b593c"
      }
    },
    "owner": "tylerlewis"
  }
}
