{
  "expo": {
    "name": "KinBoard",
    "slug": "kinboard",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/logoz.png",
    "scheme": "kinboard",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tylerlewis.kinboard",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Allow access to your camera to take profile photos, customize your circle cover images, and scan QR codes to join new circles.",
        "NSPhotoLibraryUsageDescription": "Allow access to your photo library to select and upload profile pictures and cover images for your circles.",
        "NSCalendarsUsageDescription": "Allow access to your calendar to create, sync, and manage upcoming circle events directly from the app."
      }
    },
    "android": {
      "adaptiveIcon": {
        "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_CALENDAR",
        "android.permission.WRITE_CALENDAR",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
        ],
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.tylerlewis.kinboard"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "63fd542f-1c9a-4f55-9aa4-d66bb37b593c"
      }
    },
    "owner": "tylerlewis"
  }
}
